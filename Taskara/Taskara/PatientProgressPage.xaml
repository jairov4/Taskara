<taskara:Page x:Class="Taskara.PatientProgressPage"
			 xmlns:taskara="clr-namespace:Taskara"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
			 xmlns:oxy="http://oxyplot.codeplex.com"
             mc:Ignorable="d" 
             d:DesignHeight="400" d:DesignWidth="600">
	<DockPanel>
		<Border DockPanel.Dock="Top" Background="#FF434C" Padding="20">
			<TextBlock Foreground="#FFFBED" Style="{StaticResource TitleStyle}">Reporte de Progreso</TextBlock>
		</Border>
		<Grid>
			<oxy:Plot Model="{Binding PlotModel}">
			</oxy:Plot>
		</Grid>		
	</DockPanel>
</taskara:Page>
